
mystring = 'Sammy'
for letter in mystring:
    if letter == 'a':
        continue
    print(letter)
# smmy


mystring = 'Sammy'
for letter in mystring:
    if letter == 'a':
        break
    print(letter)
#S


x = 0
while x < 5:
    if x == 2:
        break
    print(x)
    x =+ 1
# 0 1 


index_count = 0
word = 'abcde'
for letter in word:
    if letter in word == 'f':
        pass
    print(word[index_count])
    index_count += 1
